tr id="user_#{user.id}"
	= form_for @user, remote: true, html: { method: :put } do |f|
		td = f.text_field :email, form: "edit_user_#{user.id}"
		td = f.text_field :first_name, form: "edit_user_#{user.id}"
		td = f.text_field :last_name, form: "edit_user_#{user.id}"
		td = f.text_field :title, form: "edit_user_#{user.id}"
		td = f.submit class: "btn", form: "edit_user_#{user.id}"